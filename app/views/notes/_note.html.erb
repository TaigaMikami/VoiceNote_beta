<li id="note-<%= note.id %>">
  <%= link_to gravatar_for(note.user, size: 50), note.user %>
  <span class="user"><%= link_to note.user.name , note.user %></span><br>
  <span><%= image_tag note.image.url if note.image? %></span>
  <span class="content"><%= note.content %></span>
  <span class="timestamp">Posterd <%= time_ago_in_words(note.created_at) %></span>
</li>
